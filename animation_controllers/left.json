{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.left": {
      "states": {
        "default": {
          "transitions": [
            {
              "scroll": "variable.attack_time"
            }
          ],
          "on_entry": [
            "/tag @s remove hutao:left"
          ]
        },
        "scroll": {
          "transitions": [
            {
              "default": "!variable.attack_time"
            }
          ],
          "on_entry": [
            "/tag @s add hutao:left"
          ]
        }
      }
    }
  }
}